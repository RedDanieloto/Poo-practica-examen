using System;
using System.Collections.Generic;
using System.Data;
using System.Linq;
using System.Runtime.InteropServices;
using System.Text;
using System.Threading.Tasks;

namespace Examen_POO
{
    internal class cancion
    {
        string nombre, artista, album, duracion;
        int id;
        public cancion()
        {

        }
        public void setNombre(string name)
        {
            nombre = name;
        }
        public string getNombre()
        {
            return nombre;
        }
        public void setArtista(string artist)
        {
            artista = artist;
        }
        public string getArtista()
        {
            return artista;
        }
        public void setAlbum(string album_can)
        {
            album = album_can;
        }
        public string getAlbum()
        {
            return album;
        }
        public void setDuracion(string duration)
        {
            duracion = duration;
        }
        public string getDuracion()
        {
            return duracion;
        }
        public void setId(int id_can)
        {
            id = id_can;
        }
        public int getId()
        {
            return id;
        }
    }
}
